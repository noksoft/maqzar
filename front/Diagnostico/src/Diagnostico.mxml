<?xml version="1.0" encoding="utf-8"?>
<s:Application xmlns:fx="http://ns.adobe.com/mxml/2009" 
			   xmlns:s="library://ns.adobe.com/flex/spark" 
			   xmlns:mx="library://ns.adobe.com/flex/mx" minWidth="955" minHeight="600" xmlns:swiz="http://swiz.swizframework.org" xmlns:bean="com.maqzar.bean.*" xmlns:views="com.maqzar.views.*">

	<s:layout>
		<s:VerticalLayout verticalAlign="middle" horizontalAlign="center"/>
	</s:layout>
	<fx:Style source="assets/style.css"/>

	<fx:Declarations>
		<swiz:Swiz>
			<swiz:beanProviders>
				<bean:Beans/>
			</swiz:beanProviders>
			
			<swiz:config>
				<swiz:SwizConfig 
					eventPackages="com.maqzar.events.*" 
					viewPackages="com.maqzar.views.*"/>
			</swiz:config>
		</swiz:Swiz>
		
	</fx:Declarations>
	<s:ButtonBar
		width="100%"
		textAlign="center"
		dataProvider="{viewDiagnostico}">
		
	</s:ButtonBar>
	<mx:ViewStack id="viewDiagnostico" width="100%" height="100%">
		<s:NavigatorContent
			width="100%" height="100%"
			label="Diagnostico Princial">
			<views:DiagnosticoPrincipal
				width="100%" height="100%"/>
		</s:NavigatorContent>
	</mx:ViewStack>
</s:Application>
